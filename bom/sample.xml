            <forecast-period index="4" start-time-local="2015-02-26T00:00:00+11:00" end-time-local="2015-02-27T00:00:00+11:00" start-time-utc="2015-02-25T13:00:00Z" end-time-utc="2015-02-26T13:00:00Z">
